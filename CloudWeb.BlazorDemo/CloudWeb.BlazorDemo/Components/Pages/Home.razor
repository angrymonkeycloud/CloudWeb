@page "/"
@inject CloudPage CloudPage
@inject HttpClient httpClient
@{
    CloudPage.SetTitle("BB");
}

<CloudHeadContent />

<h1>Hello, world!</h1>

Welcome to your new app.

<div>
    Is Crawler: @CloudPage.IsCrawler;
</div>
@code {
    protected override void OnInitialized()
    {
        CloudPage.SetTitle("AA");
    }
    override protected void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            Console.WriteLine(httpClient);

            Console.WriteLine(CloudPage.Title);

            CloudPage.SetTitle("AA");

            Console.WriteLine(CloudPage.Title);
        }
    }
}